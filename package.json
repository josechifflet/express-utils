{
  "name": "express-utils",
  "version": "0.0.0",
  "description": "npm package that contains utilities for express.js",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prettier-check": "prettier --check src",
    "prettier-write": "prettier --write src",
    "eslint-check": "eslint src --fix",
    "type-check": "tsc --noEmit",
    "lint": "yarn prettier-check && yarn eslint-check && yarn type-check",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/josechifflet/express-utils"
  },
  "keywords": [
    "express",
    "utils",
    "jwt",
    "rate-limit",
    "slow-down"
  ],
  "author": "Jose Pedro Chifflet",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/josechifflet/express-utils/issues"
  },
  "homepage": "https://github.com/josechifflet/express-utils#readme",
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@tsconfig/node22": "^22.0.0",
    "@types/express": "^5.0.0",
    "@types/express-slow-down": "^1.3.2",
    "@types/hpp": "^0.2.2",
    "@types/morgan": "^1.9.3",
    "@types/node": "^22.9.0",
    "@types/request-ip": "^0.0.41",
    "@types/toobusy-js": "^0.5.2",
    "dpdm": "^3.9.0",
    "eslint": "9.14.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^15.12.0",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.13.0",
    "vitest": "^2.1.4"
  },
  "dependencies": {
    "@types/winston": "^2.4.4",
    "argon2": "^0.41.1",
    "device-detector-js": "^3.0.3",
    "express": "^4.21.1",
    "express-jwt": "^8.4.1",
    "express-slow-down": "^2.0.3",
    "express-winston": "^4.2.0",
    "helmet": "^8.0.0",
    "hpp": "^0.2.3",
    "http-status-codes": "^2.3.0",
    "ioredis": "^5.4.1",
    "jose": "^5.9.6",
    "morgan": "^1.10.0",
    "otpauth": "^9.3.4",
    "rate-limit-redis": "^4.2.0",
    "winston": "^3.16.0",
    "zod": "^3.23.8"
  },
  "files": [
    "lib/**/*"
  ]
}
